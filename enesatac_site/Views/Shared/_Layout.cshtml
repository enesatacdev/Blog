<!DOCTYPE html>
<html lang="en-US">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta http-equiv="content-type" content="text/html;charset=UTF-8" />
    <title>@ViewBag.Title | enesatac.com</title>
    <meta name="description" content="@ViewBag.MetaKeywords" />
    <meta name="keywords" content="@ViewBag.MetaDescription" />
    <meta name="viewport" content="width=device-width,minimum-scale=1,maximum-scale=1,user-scalable=no" />
    <link rel="icon" href="~/Content/images/favicon.png" type="image/x-icon" />
    <link rel="stylesheet" href="http://fonts.googleapis.com/css?family=Open+Sans%3A400italic%2C700italic%2C700%2C400%7CRoboto+Slab&amp;ver=5.5.3" type="text/css" />
    <link rel="stylesheet" href="~/Content/css/bootstrap.min.css" type="text/css" />
    <link rel="stylesheet" href="~/Content/css/font-awesome.min.css" type="text/css" />
    <link rel="stylesheet" href="~/Content/css/screen.css" type="text/css" />
    <link rel="stylesheet" href="~/Content/css/sliderstyler.css" type="text/css" />
    <script type="text/javascript" src="~/Content/js/jquery4a5f.js?ver=1.12.4-wp" id="jquery-core-js"></script>
    <script type="text/javascript" src="~/Content/js/highlight.pack68b3.js?ver=1" id="highlight-js-js"></script>
    <link rel="stylesheet" href="~/Content/css/splide.min.css" type="text/css" />
    <link href="~/Content/DataTables/css/dataTables.bootstrap4.css" rel="stylesheet" />
</head>
<body class="home blog">
    <header class="main-header">
        <div class="container">
            <div class="row">
                <div class="col-sm-12">
                    <a class="branding" href="#"><img src="~/Content/images/logo.png" alt="Enes Ataç"></a>
                </div>
            </div>
        </div>
    </header>

    <nav class="main-navigation sticky-sidebars">
        <div class="container">
            <div class="row">
                <div class="col-sm-12">
                    <div class="navbar-header">
                        <span class="nav-toggle-button collapsed" data-toggle="collapse" data-target="#main-menu">
                            <span class="sr-only">Menü</span>
                            <i class="fa fa-bars"></i>
                        </span>
                    </div>
                    <div class="collapse navbar-collapse" id="main-menu">
                        <ul id="menu-primary-menu" class="menu">
                            <li class="menu-item menu-item-type-custom menu-item-object-custom"><a href="/Home/Index">Anasayfa</a></li>
                            @Html.Action("HeaderNavigation", "Widget")
                            <li class="menu-item menu-item-type-custom menu-item-object-custom"><a href="/yazar/detay/6/enes">İletişim</a></li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </nav>

    @RenderBody()


    <footer class="main-footer">
        <div class="container">
            <div class="row">



                @Html.Action("FooterNavigation", "Widget")

                @Html.Action("FooterPopularArticles", "Widget")

                @Html.Action("FooterLastArticles", "Widget")


            </div>
        </div>
    </footer>


    <div class="copyright">
        <div class="container">
            <div class="row">
                <div class="col-sm-12">
                    &copy; 2021 Tüm Hakları Saklıdır. <a href="/yazar/detay/6/enes">Enes Ataç</a>.&nbsp;
                </div>
            </div>
        </div>
    </div>

    <a href="#" id="back-to-top"><i class="fa fa-angle-up"></i></a>
    <script type="text/javascript" src="~/Content/js/jquery.fitvids5697.js"></script>
    <script type="text/javascript" src="~/Content/js/slider.js"></script>
    <script type="text/javascript" src="~/Content/js/bootstrap.min5697.js"></script>
    <script type="text/javascript" src="~/Content/js/jquery.flexslider-min5697.js"></script>
    <script type="text/javascript" src="~/Content/js/jquery.magnific-popup.min5697.js"></script>
    <script type="text/javascript" src="~/Content/js/main5697.js"></script>
    <script type="text/javascript" src="~/Content/js/jflickrfeed.min5697.js"></script>
    <script type="text/javascript" src="~/Content/js/splide.min.js"></script>
    <script>
        document.addEventListener('DOMContentLoaded', function () {
            new Splide('.splide').mount();
        });
    </script>
    <script src="https://code.jquery.com/jquery-3.5.1.min.js"
            integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0="
            crossorigin="anonymous"></script>
    <script>
        $(document).ready(function begeni() {
            $("a.begen").click(function begeni() {
                var id = $(this).data("id");
                var link = "/article/like/" + id;
                $.ajax({
                    type: "GET",
                    url: link,
                    success: function (result) {
                        $("a.begen span").html(result);
                    }
                })
            });

        });
    </script>

</body>


</html>









